source ~/.vimrc

set number

map ,wh :action VimWindowSplitHorizontal<CR>
map ,wv :action VimWindowSplitVertical<CR>
map ,wx :action VimWindowClose<CR>
map ,wo :action VimWindowOnly<CR>

map ,h :action VimWindowLeft<CR>
map ,j :action VimWindowDown<CR>
map ,k :action VimWindowUp<CR>
map ,l :action VimWindowRight<CR>


"MoveEditorToOppositeTabGroup
